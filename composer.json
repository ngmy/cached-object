    "keywords": [
        "laravel 4",
        "cache"
    ],
    "homepage": "https://github.com/ngmy/cached-object",
    "license": "MIT",
    "authors": [
        {
            "name": "ngmy",
            "email": "y.nagamiya@gmail.com"
        }
    ],
    "require": {
